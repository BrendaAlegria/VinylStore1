<%- include ("../templates/encaInici")%>
<% if (tipo == undefined) { %>
    <%- include ("../templates/menuAdmin")%>
<% } else { %>
    <%- include ("../templates/menuUsuarios")%>
<% } %>
<header>
    <h1>Vinilos que me Gustan</h1>
</header>
<br>
<div class="gallery">
    <% discosMeGusta.forEach((disco) => { %>
        <!-- Aquí muestra la información del vinilo que le gusta al usuario -->
        <div class="image">
            <img src="/images/<%= disco.foto %>" alt="<%= disco.nombre %>" class="imagen-disco">
            <div class="card-body">
                <h4 class="card-title custom-font"><%= disco.nombre %></h4>
                <p class="card-text custom-font">Artista: <%= disco.artista %></p>
                <p class="card-text custom-font">Duración: <%= disco.duracion %></p>
                <p class="card-text custom-font">Precio: <%= disco.precio %></p>
            </div>
        </div>
    <% }); %>
</div>
<%- include ("../templates/pie")%>